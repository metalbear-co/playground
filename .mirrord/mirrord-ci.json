{
  "target": {
    "namespace": "ip-visit-counter",
    "path": {
      "deployment": "ip-visit-counter"
    }
  },
  "ci": {
    "output_dir": "/tmp/mirrord"
  },
  "feature": {
    "network": {
      "incoming": {
