{
  "target": {
    "namespace": "visualization-shop",
    "path": {
      "deployment": "visualization-shop-frontend"
    }
  },
  "feature": {
    "env": {
      "exclude": [
        "NODE_ENV",
        "NX_NEXT_DIR"
      ],
      "override": {
        "DB_BRANCH_MOCK_DATA": "false",
        "QUEUE_SPLITTING_MOCK_DATA": "false",
        "MULTIPLE_SESSION_MOCK_DATA": "true"
      }
    },
    "network": {
      "incoming": {
        "mode": "steal",
        "http_filter": {
          "any_of": [
            { "header": "X-PG-Tenant: Ari" }
          ]
        }
      },
      "outgoing": {
        "ignore_localhost": true
      }
    }
  }

}
