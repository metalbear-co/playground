{
  "target": {
    "path": "deploy/inventory-service",
    "namespace": "shop"
  },
  "feature": {
    "network": {
      "incoming": {
        "mode": "steal",
        "http_filter": {
          "header_filter": "X-PG-Tenant: dev"
        }
      },
      "outgoing": true
    },
    "db_branches": [
      {
        "id": "metal-mart-dev",
        "type": "pg",
        "version": "16",
        "name": "inventory",
        "connection": {
          "url": {
            "type": "env",
            "variable": "DATABASE_URL"
          }
        }
      }
    ]
  }
}
