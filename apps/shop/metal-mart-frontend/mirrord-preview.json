{
  "target": {
    "namespace": "shop",
    "path": {
      "deployment": "metal-mart-frontend"
    }
  },
  "feature": {
    "preview": {
      "ttl_mins": 120,
      "creation_timeout_secs": 600
    },
    "network": {
      "incoming": {
        "mode": "steal",
        "http_filter": {
          "header_filter": "X-PG-Tenant: {{ key }}"
        }
      },
      "outgoing": {
        "ignore_localhost": true
      }
    },
    "env": {
      "override": {
        "NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME": "dxas4fpir"
      },
      "exclude": "NODE_ENV"
    }
  }
}
