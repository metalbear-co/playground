{
  "target": {
    "path": {
      "deployment": "order-service"
    }
  },
  "feature": {
    "network": {
      "incoming": {
        "mode": "steal",
        "http_filter": {
          "header_filter": "X-PG-Tenant: dev"
        }
      },
      "outgoing": true
    },
    "db_branches": [
      {
        "id": "metal-mart-dev",
        "type": "postgres",
        "version": "16",
        "name": "orders",
        "connection": {
          "url": {
            "type": "env",
            "variable": "DATABASE_URL"
          }
        }
      }
    ]
  }
}
