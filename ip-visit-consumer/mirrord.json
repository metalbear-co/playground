{
    "accept_invalid_certificates": false,
    "target": {
        "path": {
            "deployment": "ip-visit-consumer"
        }
    },
    "feature": {
        "network": {
            "incoming": "steal",
            "outgoing": true
        },
        "fs": {
            "mode": "read"
        },
        "split_queues": {
            "ip-visits": {
                "queue_type": "Kafka",
                "message_filter": {
                    "x-pg-tenant": "Avi."
                }
            }
        }
    }
}
