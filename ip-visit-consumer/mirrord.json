{
    "accept_invalid_certificates": false,
    "target": {
        "path": {
            "deployment": "ip-visit-consumer"
        }
    },
    "feature": {
        "network": {
            "incoming": {
                "mode": "steal",
                "http_filter": {
                    "header_filter": "x-pg-tenant: Avi"
                }
            },
            "outgoing": true
        },
        "split_queues": {
            "ip-visits": {
                "queue_type": "Kafka",
                "message_filter": {
                    "x-pg-tenant": "Avi"
                }
            }
        }
    }
}
